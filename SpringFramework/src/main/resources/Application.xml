<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="Person" name="person12345" class="com.zhang.model.Person">
        
    </bean>
    <bean id="PersonLife" class="com.zhang.model.PersonLife">
        <property name="person" ref="person2"/>
    </bean>
    <alias name="person12345" alias="person"/>
    <alias name="person12345" alias="person2"/>
</beans>
